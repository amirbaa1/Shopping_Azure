{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/basket/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/basket/{userId}",
      "UpstreamHttpMethod": [
        "GET"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Basket?UserId={userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/Basket?UserId={userId}",
      "UpstreamHttpMethod": [
        "POST"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Basket?itemId={itemId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/Basket?itemId={itemId}",
      "UpstreamHttpMethod": [
        "DELETE"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Basket?basketItemId={basketItemId}&quantity={quantity}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/Basket?basketItemId={basketItemId}&quantity={quantity}",
      "UpstreamHttpMethod": [
        "PUT"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Basket/{basketId}/{discountId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/Basket/{basketId}/{discountId}",
      "UpstreamHttpMethod": [
        "PUT"
      ]
    },
    {
      "DownstreamPathTemplate": "/api/Basket/CheckOutBasket",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "7002"
        }
      ],
      "UpstreamPathTemplate": "/Basket/CheckOutBasket",
      "UpstreamHttpMethod": [
        "POST"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https//localhost:7070"
  }
}